<mvc:View
    controllerName="com.kpo.sm.eddapplication.controller.SupplierEDD"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
        
        
   
                 
                  
           
            <Table
                id="id_CompanyTable"
                items="{path:'sModel>/questions'}"
                 
                 width="100%"
            >
                <columns>
                    <Column id="_IDGenColumn11" width="5rem" >
                        <Text
                            id="_IDGenText22"
                            text="SN"
                           
                            
                        />
                    </Column>
                    <Column id="_IDGenColumn1" width="15rem"  >
                        <Text
                            id="_IDGenText2"
                            text="Title"
                            
                        />
                    </Column>
                    <Column id="_IDGenColumn12" width="10rem"  >
                        <Text
                            id="_IDGenText23"
                            text="Response"
                           
                            
                        />
                   </Column>
                   <Column id="_IDGenColumn13" width="15rem" >
                        <Text
                            id="_IDGenText24"
                            text="Description"
                           
                            
                        />
                   </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1">
                        <cells>
                            <Text
                                id="_IDGenText60"
                                text="{sModel>supplierid}"
                            />
                            <Text text="{path: 'sModel>questionKey',  formatter: '.i18nFormatterSupplier'}" />
                            <RadioButtonGroup
                                select="onSelect"
                                columns="2"
                                id="radiobuttonid"
                            >
                                <RadioButton
                                    text="yes"
                                    selected="false"
                                    id="yes"
                                />
                                <RadioButton text="no"
                                id="no" />
                            </RadioButtonGroup>
                            <TextArea  />
                            
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
        
    </Page>
</mvc:View>
