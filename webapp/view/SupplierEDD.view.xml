<mvc:View
    controllerName="com.kpo.sm.eddapplication.controller.SupplierEDD"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:f="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
    xmlns:upload="sap.m.upload"
    xmlns="sap.m"
>
    <Page
        id="page1"
        title="{i18n>title}"
    >
        <content>
            <uxap:ObjectPageLayout
                id="ObjectPageLayout"
                upperCaseAnchorBar="false"
                showAnchorBar="true"
                enableLazyLoading="true"
            >
                <uxap:sections>
                    <uxap:ObjectPageSection
                        id="_IDGenObjectPageSection1"
                        showTitle="false"
                    >
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection1"
                                title="{i18n>ques} "
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_SupplierTable"
                                        items="{path:'sModel>/questions'}"
                                        width="100%"
                                    >
                                        <columns>
                                            <Column
                                                id="_IDGenColumn1011"
                                                width="5rem"
                                            >
                                                <Text
                                                    id="_IDGenText022"
                                                    text="{i18n>sn}"
                                                />
                                            </Column>
                                            <Column id="_IDGenColumn1111">
                                                <Text
                                                    id="_IDGenText232"
                                                    text="{i18n>questiontitle}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn12"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenText23"
                                                    text="{i18n>response}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn13"
                                                width="15rem"
                                            >
                                                <Text
                                                    id="_IDGenText24"
                                                    text="{i18n>desc}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn14"
                                                width="15rem"
                                            >
                                                <Text
                                                    id="_IDGenText25"
                                                    text="{i18n>res}"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="_IDGenColumnListItem12"
                                            >
                                                <cells>
                                                    <Text
                                                        id="_IDGenText6012"
                                                        text="{sModel>supplierid}"
                                                    />
                                                    <Text
                                                        text="{path: 'sModel>questionKey',  formatter: '.i18nFormatterSupplier'}"
                                                    />
                                                    <RadioButtonGroup
                                                        select="onSelect"
                                                        columns="2"
                                                        id="radiobuttonid"
                                                    >
                                                        <RadioButton
                                                            text="{i18n>yes}"
                                                            selected="false"
                                                            id="yes"
                                                        />
                                                        <RadioButton
                                                            text="{i18n>no}"
                                                            id="no"
                                                        />
                                                    </RadioButtonGroup>
                                                    <TextArea cols="40" />
                                                    <Link
                                                        text="{sModel>link}"
                                                        href="{sModel>link}"
                                                        target="_blank"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection525"
                                title="{i18n>cetificateofcorporateownershipanddeclaration}"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_Table2"
                                        width="100%"
                                        mode="SingleSelectLeft"
                                        items="{oTable1Model>/}"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar>
                                                <Title
                                                    id="idTableTitle"
                                                    text="{i18n>detailsofdirectorsandofficersofcompany}"
                                                />

                                                <ToolbarSpacer />

                                                <Button
                                                    id="idAddButton"
                                                    text="{i18n>add}"
                                                    type="Transparent"
                                                    icon="sap-icon://add"
                                                    press="onAddPressTable1"
                                                />

                                                <Button
                                                    id="idDeleteButton"
                                                    text="{i18n>delete}"
                                                    type="Transparent"
                                                    icon="sap-icon://delete"
                                                    press="onDeletePressTable1"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column
                                                id="_IDGenCo1"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTe1"
                                                    text="{i18n>fullName}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColum1"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTex11"
                                                    text="{i18n>dateOfBirth}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn21"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGnTe11"
                                                    text="{i18n>positionandDateofappointment}"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="_IDGenColumnItem12"
                                            >
                                                <cells>
                                                    <Input
                                                        id="_IDGeext2"
                                                        value="{oTable1Model>fullname}"
                                                    />
                                                    <Input
                                                        value="{oTable1Model>dateofbirth}"
                                                    />
                                                   <Input
                                                        value="{oTable1Model>dateofappointment}"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection5256"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_Table4"
                                        width="100%"
                                        mode="SingleSelectLeft"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar>
                                                <Title
                                                    id="idTableTitle1"
                                                    text="{i18n>detailsofshareholdersandownership}"
                                                />

                                                <ToolbarSpacer />

                                                <Button
                                                    id="idAddButton1"
                                                    text="{i18n>add}"
                                                    type="Transparent"
                                                    icon="sap-icon://add"
                                                />

                                                <Button
                                                    id="idDeleteButton1"
                                                    text="{i18n>delete}"
                                                    type="Transparent"
                                                    icon="sap-icon://delete"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column
                                                id="_IDGenCo12"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTe01"
                                                    text="{i18n>FullName}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColum11110"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTx11"
                                                    text="{i18n>DateofBirth}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenCoum19"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGne11"
                                                    text="{i18n>detailsofshareholdingandownership}"
                                                />
                                            </Column>
                                        </columns>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection525o"
                                title="{i18n>declarationofcompliance}"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_Table02"
                                        width="100%"
                                        mode="SingleSelectLeft"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar>
                                                <Title
                                                    id="idTableTitle03"
                                                    text="{i18n>detailsofrelationshipsofgovtofficialskpostaff}"
                                                />

                                                <ToolbarSpacer />

                                                <Button
                                                    id="idAddButton02"
                                                    text="{i18n>add}"
                                                    type="Transparent"
                                                    icon="sap-icon://add"
                                                    press="onAddPress"
                                                />

                                                <Button
                                                    id="idDeleteButton01"
                                                    text="{i18n>delete}"
                                                    type="Transparent"
                                                    icon="sap-icon://delete"
                                                    press="onDeletePress"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column
                                                id="_IDGenCo11"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTe10"
                                                    text="{i18n>fullNameofshareholderowner}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenCoum1"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTex110"
                                                    text="{i18n>detailsofshareholdingandownership}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn210"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGnTe110"
                                                    text="{i18n>detailsofgovernmentofficialkpostaffandtypeofrelationship}"
                                                />
                                            </Column>
                                        </columns>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection52565"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_Table6"
                                        width="100%"
                                        mode="SingleSelectLeft"
                                    >
                                        <headerToolbar>
                                            <OverflowToolbar>
                                                <Title
                                                    id="idTableTitle2"
                                                    text="{i18n>detailsofrelationshipswithgovernmentofficialsandkpoemployee}"
                                                />

                                                <ToolbarSpacer />

                                                <Button
                                                    id="idAddButton2"
                                                    text="{i18n>add}"
                                                    type="Transparent"
                                                    icon="sap-icon://add"
                                                />

                                                <Button
                                                    id="idDeleteButton2"
                                                    text="{i18n>delete}"
                                                    type="Transparent"
                                                    icon="sap-icon://delete"
                                                />
                                            </OverflowToolbar>
                                        </headerToolbar>
                                        <columns>
                                            <Column
                                                id="_IDGenCo120"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTe011"
                                                    text="{i18n>relationship}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColum111101"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTx111"
                                                    text="{i18n>detailsofgovernmentofficials}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenCoum10"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGne110"
                                                    text="{i18n>detailsofkpoemployee}"
                                                />
                                            </Column>
                                        </columns>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection2000"
                                title="{i18n>complianceEthicalConductLetterEsign}"
                            >
                                <uxap:blocks>
                                    <Table
                                        id="id_SupplierTable1"
                                        items="{stModel>/}"
                                        width="100%"
                                        mode="SingleSelectLeft"
                                    >
                                        <columns>
                                            <Column
                                                id="_IDGenCo"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTe"
                                                    text="{i18n>templetetitle}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColum"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGenTex"
                                                    text="{i18n>previewtemplate}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenColumn2"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGnTe"
                                                    text="{i18n>downloadtemplate}"
                                                />
                                            </Column>
                                            <Column
                                                id="_IDGenCon13"
                                                width="10rem"
                                            >
                                                <Text
                                                    id="_IDGet24"
                                                    text="{i18n>esign}"
                                                />
                                            </Column>
                                        </columns>
                                        <items>
                                            <ColumnListItem
                                                id="_IDGenCoistItem1"
                                            >
                                                <cells>
                                                    <Text
                                                        id="_IDGenText0"
                                                        text="{stModel>documentTitle}"
                                                    />
                                                    <Button
                                                        text="Preview"
                                                        press="onPreview"
                                                    />
                                                    <Button
                                                        text="Download Templete"
                                                        press="onDownload"
                                                    />
                                                    <Button
                                                        text="E-sign"
                                                        press="onSignButtonPress"
                                                    />
                                                </cells>
                                            </ColumnListItem>
                                        </items>
                                    </Table>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>

                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection2"
                                title="{i18n>attachment}"
                            >
                                <uxap:blocks>
                                    <upload:UploadSet
                                        id="uploadSet"
                                        instantUpload="false"
                                        uploadEnabled="true"
                                        showIcons="true"
                                        afterItemAdded="onAfterItemAdded"
                                        uploadCompleted="onUploadCompleted"
                                        items="{
                                path: '/Files',
                                parameters: {
                                    $orderby: 'createdAt desc'
                                },
                                templateShareable: false}"
                                    >
                                        <!-- <upload:toolbar>
                    <OverflowToolbar>
                        
                        <Title text="Attachment"/>
                    </OverflowToolbar>
                </upload:toolbar> -->

                                        <upload:items>
                                            <upload:UploadSetItem
                                                fileName="{fileName}"
                                                mediaType="{mediaType}"
                                                url="{url}"
                                                thumbnailUrl="{
                                path: 'mediaType',
                                formatter: '.formatThumbnailUrl'
                            }"
                                                enabledEdit="false"
                                                openPressed="onOpenPressed"
                                                removePressed="onRemovePressed"
                                            >
                                                <upload:attributes>
                                                    <ObjectAttribute
                                                        title="Uploaded By"
                                                        text="{createdBy}"
                                                        active="false"
                                                    />
                                                    <ObjectAttribute
                                                        title="Uploaded on"
                                                        text="{createdAt}"
                                                        active="false"
                                                    />
                                                    <ObjectAttribute
                                                        title="File Type"
                                                        text="{mediaType}"
                                                        active="false"
                                                    />
                                                    <ObjectAttribute
                                                        title="File Size"
                                                        text="{size}"
                                                        active="false"
                                                    />
                                                </upload:attributes>
                                            </upload:UploadSetItem>
                                        </upload:items>
                                    </upload:UploadSet>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                </uxap:sections>
            </uxap:ObjectPageLayout>
        </content>
        <footer>
            <OverflowToolbar id="Toolbar12">
                <ToolbarSpacer />

                <Button
                    type="Emphasized"
                    text="{i18n>submit}"
                    press="onSubmit"
                />

                <Button
                    type="Reject"
                    text="{i18n>cancel}"
                />
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>
